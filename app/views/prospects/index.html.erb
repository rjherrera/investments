<p id="notice"><%= notice %></p>

<h1>Prospects</h1>

<table class="ui celled table">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>RUT</th>
      <th>Teléfono</th>
      <th>Email</th>
      <th>Rango de renta</th>
      <th>Preaprobación</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prospects.each do |prospect| %>
      <tr>
        <td><%= prospect.name %></td>
        <td><%= prospect.last_name %></td>
        <td><%= prospect.rut %></td>
        <td><%= prospect.cellphone %></td>
        <td><%= prospect.email %></td>
        <td><%= prospect.salary %></td>
        <td><%= prospect.preapproval %></td>
        <td><%= link_to 'Show', prospect %></td>
        <td><%= link_to 'Edit', edit_prospect_path(prospect) %></td>
        <td><%= link_to 'Destroy', prospect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prospect', new_prospect_path %>
